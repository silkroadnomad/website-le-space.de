{
	"name": "website-le-space.de",
	"version": "0.1.7",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:ipfs": "vite build && chmod +x ./fix_build.sh && sh fix_build.sh",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@beyonk/gdpr-cookie-consent-banner": "^12.1.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/kit": "^1.27.6",
		"alchemy-sdk": "^3.1.0",
		"carbon-components-svelte": "^0.81.0",
		"carbon-icons-svelte": "^12.3.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.7",
		"svelte-carousel": "^1.0.25",
		"svelte-check": "^3.6.2",
		"svelte-flag-icons": "^0.7.2",
		"svelte-gestures": "^1.5.2",
		"svelte-i18n": "^4.0.0",
		"sveltejs-adapter-ipfs": "^0.4.12",
		"typescript": "^5.0.0",
		"vite": "^4.5.0",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"@helia/unixfs": "^1.4.3",
		"ethers": "^6.9.0",
		"helia": "^2.1.0",
		"svelte-marquee-text-widget": "^1.1.0"
	}
}
